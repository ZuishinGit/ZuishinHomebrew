		{
			"name": "Yaksa",
			"source": "ZPSN",
			"type": "G",
			"rarity": "none",
			"value": 170000,
			"poison": true,
			"entries": [
				{
					"type":"entries",
					"name":"Appearance",
					"entries":[
						"White liquid or powder that smells like cherries."
					]
				},
				{
					"type":"entries",
					"name":"Effect",
					"entries":[
						"After 4 ({@dice 1d8}) rounds, the creature becomes {@condition poisoned} for 1 minute. At the start of each of the creature's turns they must succeed on a DC 13 Constitution saving throw or take 18 ({@dice 4d8}) poison damage. After three successful saves, the poison ends."
					]
				}
			],
			"poisonTypes": [
				"ingested"
			]
		},
